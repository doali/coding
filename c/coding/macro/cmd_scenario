#!/usr/bin/env bash

# clean
./cmd_clean

# [preprocessor] => .i
cpp macro.c >macro.i

# assemblee code => .s
gcc -S macro.i 

# object code => .o
as -o macro.o macro.s 

# interface
nm macro.o 

# linked (ld) => .exe
gcc -o macro.exe macro.o 

# dependencies
ldd macro.o
ldd macro.exe

# run
#./macro 
